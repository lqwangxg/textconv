rules:
- pattern: \s+(\w*[a-z]+\w*)\s*\w*\s*(,\s*(\w*[a-z]+\w*)\s*\w*)*
  - groups: 1,3
    func: ''
  rangeFrom: FROM
  rangeTo: WHERE
  dicwordfile: repfiles/tablename.txt
  fileSkip: true
  filefilter: \w+epsDBAccess.js

- pattern: (?:FROM|UPDATE|DELETE(?:\s+FROM)*|INSERT\s+INTO)\s+(\w*[a-z]+\w*)\s*\w*\s*(,\s*(\w*[a-z]+\w*)\s*\w*)*
  - groups: 1,3
    func: ''
  rangeFrom: FROM
  rangeTo: WHERE
  dicwordfile: repfiles/tablename.txt
  fileSkip: true
  filefilter: \w+epsDBAccess.js
